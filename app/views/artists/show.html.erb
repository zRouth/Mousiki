<div class="welcome-user">
  <%= "Welcome #{current_user.name}" %>
</div>
<div class="user2">
  Here is the Calendar for <%= @calendar.first['performance'].first['artist']['displayName'] %>
</div>




<div class="artist-list">
<div class="row" id="artist-list2">
  <table>
    <thead>
    <tr>
      <th width="200">Artist</th>
      <th>Venue and Date</th>
      <th width="150">Country</th>
      <th width="150">Buy Tickets</th>
    </tr>
  </thead>
     <tbody>
      <tr>
        <% @calendar.each do |event| %>
          <td>Artist: <%= event["performance"].first["artist"]["displayName"] %></td>
          <td>Venue: <%= event["displayName"] %></td>
          <td>Country: <%= event["venue"]["metroArea"]["country"]["displayName"]%>
          <%= event["venue"]["metroArea"]["displayName"] %></td>
      </tr>
          <% end %>
  </div>
</div>
